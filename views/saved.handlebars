<div class="container">

    <h1 class="text-center">{{headLine}}</h1>
    <br>

    {{#each ArticleInfo}}
    <ol>
        <div class="card art{{id}}" data-id={{id}}>
            <div class="card-header">
                <a target="_blank" href="{{this.link}}">
                    <h3>{{this.title}}</h3>
                </a>
            </div>
            <div class="card-body">
                <p class="card-text" data-id={{id}}>
                    {{this.summary}}
                </p>
                <button data-whatever="aaa" type="button" data-toggle="modal" data-target="#exampleModal" class="btn btn-primary articleNote"
                    data-id={{id}} id={{id}}>Article Note</button>
                <button class="btn btn-success deteleSaved" data-id={{id}} id={{id}}>Detele From Saved List</button>
            </div>
        </div>
        <br class="card art{{id}}" data-id={{id}}>

    </ol>

      {{/each}}


    {{!-- <div id="notes"></div> --}}
</div>

{{>Modal }}





